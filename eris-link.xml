<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-eris-link+cbor">
    <comment xml:lang="en">ERIS link</comment>
    <comment xml:lang="de">ERIS Verbindung</comment>
    <acronym>ERIS</acronym>
    <expanded-acronym xml:lang="en">Encoding for Robust Immutable Storage</expanded-acronym>
    <glob pattern="*.eris"/>
    <magic>
      <match type="string" value="\xD9\xD9\xF7\x84\xD9\x01\x14\x58\x42" offset="0"/>
    </magic>
    <icon name="eris"/>
  </mime-type>
  <mime-type type="x-scheme-handler/urn:eris">
    <comment xml:lang="en">ERIS capability URN</comment>
    <comment xml:lang="de">ERIS Berechtigung URN</comment>
  </mime-type>
  <mime-type type="application/x-eris-fs">
    <comment xml:lang="en">ERIS-FS image</comment>
    <acronym>ERIS</acronym>
    <expanded-acronym xml:lang="en">Encoding for Robust Immutable Storage</expanded-acronym>
    <glob pattern="*.eris-fs"/>
    <magic>
      <match type="string" value="\xDA\x65\x72\x69\x74\x82" offset="0"/>
    </magic>
    <icon name="eris"/>
  </mime-type>
</mime-info>
